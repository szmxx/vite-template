{
  "name": "vite-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,vue,tsx --ignore-path .gitignore .",
    "style": "stylelint src/**/*.{css,scss,vue}",
    "lint:markup": "vue-tsc --noEmit",
    "commit": "git-cz",
    "test": "vitest",
    "ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@vueuse/core": "^8.9.0",
    "axios": "^0.27.2",
    "element-plus": "^2.2.8",
    "lodash": "^4.17.21",
    "pinia": "^2.0.0-rc.10",
    "vue": "^3.2.25",
    "vue-i18n": "^9.2.0-beta.36",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/cz-commitlint": "^17.0.3",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vitest/ui": "^0.17.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "c8": "^7.11.3",
    "commitizen": "^4.2.4",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.4.1",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.5.4",
    "unocss": "^0.43.2",
    "unocss-preset-scrollbar": "^0.2.1",
    "unplugin-auto-import": "^0.9.2",
    "unplugin-vue-components": "^0.21.1",
    "unplugin-vue-define-options": "^0.6.1",
    "vite": "^2.9.9",
    "vite-plugin-inspect": "^0.5.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.17.0",
    "vue-tsc": "^0.34.7"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "npm run lint --fix",
    "*.{less,sass,css,scss,vue}": "npm run style --fix",
    "src/**/*.{json,md,vue}": "prettier -w -u",
    "docs/**/*.{json,md,vue}": "prettier -w -u"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
