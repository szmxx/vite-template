{
  "name": "vite-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,vue,tsx --ignore-path .gitignore .",
    "style": "stylelint src/**/*.{css,scss,vue}",
    "lint:markup": "vue-tsc --noEmit",
    "commit": "git-cz",
    "test": "vitest",
    "ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.1.6",
    "@vueuse/core": "^8.9.4",
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "pinia": "^2.0.16",
    "vue": "^3.2.37",
    "vue-request": "^2.0.0-rc.3",
    "vue-router": "^4.1.2",
    "vue-i18n": "^9.2.0-beta.36"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/cz-commitlint": "^17.0.3",
    "@iconify-json/mdi": "^1.1.29",
    "@types/jsdom": "^16.2.15",
    "@types/node": "^18.0.6",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@unocss/preset-icons": "^0.44.5",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vitest/ui": "^0.18.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "c8": "^7.12.0",
    "commitizen": "^4.2.5",
    "eslint": "^8.20.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "terser": "^5.14.2",
    "typescript": "^4.7.4",
    "unocss": "^0.44.5",
    "unocss-preset-scrollbar": "^0.2.1",
    "unplugin-auto-import": "^0.9.4",
    "unplugin-icons": "^0.14.7",
    "unplugin-vue-components": "^0.21.1",
    "unplugin-vue-define-options": "^0.6.2",
    "vite": "^3.0.2",
    "vite-plugin-inspect": "^0.6.0",
    "vitest": "^0.18.1",
    "vue-tsc": "^0.38.9"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "npm run lint --fix",
    "*.{less,sass,css,scss,vue}": "npm run style --fix",
    "src/**/*.{json,md,vue}": "prettier -w -u",
    "docs/**/*.{json,md,vue}": "prettier -w -u"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
